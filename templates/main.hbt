<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Metalsmith - Slides</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,300italic,700|Sanchez" rel="stylesheet">
    <link rel="stylesheet" href="styles/main.css">
</head>
<body>
    
    <div class="reveal">
        <div class="slides">
            {{#each collections.slides}}
                <section class="slide {{klass}}">
                    {{{ contents }}}
                </section>
            {{/each}}     
        </div>        
    </div>

    <script src="scripts/reveal.min.js"></script>
    <script src="scripts/main.js"></script>
</body>
</html>